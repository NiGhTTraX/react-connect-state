{
  "name": "react-connect-state",
  "description": "Simple, type safe and time travelling state management for React",
  "keywords": [
    "react",
    "state",
    "typescript"
  ],
  "version": "4.0.0",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "playground": "cosmos --config playground/cosmos.config.js",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc --project tsconfig.build.json --declaration --outDir ./dist",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext ts,tsx,js .",
    "test": "./tests/run.sh",
    "test:node": "TS_NODE_CACHE=0 mocha --opts tests/node/mocha.opts",
    "test:node:coverage": "TS_NODE_CACHE=0 COVERAGE=1 nyc mocha --opts tests/node/mocha.coverage.opts",
    "test:gui": "./tests/gui/run.sh",
    "test:gui:debug": "./tests/gui/debug.sh",
    "test:gui:selenium": "./tests/gui/debug-selenium.sh",
    "test:gui:selenium:stop": "docker-compose -f tests/gui/docker-compose.debug.yml down",
    "_test:gui:fixtures": "cd playground && TS_NODE_CACHE=0 ./get-fixtures.js",
    "_test:gui": "TS_NODE_CACHE=0 mocha --opts tests/gui/mocha.opts",
    "_test:gui:debug": "TS_NODE_CACHE=0 SELENIUM_HOST=localhost mocha --opts tests/gui/mocha.debug.opts",
    "report-coverage": "nyc report --reporter=json && codecov -f tests/results/coverage/coverage-final.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx?": "eslint",
    "*.js": "eslint"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:NiGhTTraX/react-connect-state.git"
  },
  "dependencies": {
    "@material-ui/core": "~3.9.2",
    "react-bind-component": "~1.0.1",
    "stringify-object": "~3.3.0"
  },
  "peerDependencies": {
    "react": "^16"
  },
  "devDependencies": {
    "@babel/core": "~7.4.0",
    "@babel/plugin-proposal-class-properties": "~7.4.0",
    "@babel/plugin-proposal-decorators": "~7.4.0",
    "@babel/preset-env": "~7.4.0",
    "@babel/preset-react": "~7.0.0",
    "@babel/preset-typescript": "~7.3.0",
    "@babel/register": "~7.4.0",
    "@nighttrax/eslint-config-tsx": "~1.0.0",
    "@types/chai": "~4.1.7",
    "@types/fetch-mock": "~7.2.1",
    "@types/jquery": "~3.3.22",
    "@types/jsdom": "~12.2.0",
    "@types/mocha": "~5.2.5",
    "@types/node": "~10.12.2",
    "@types/react": "16.8.18",
    "@types/react-dom": "~16.8.2",
    "@types/react-hot-loader": "~4.1.0",
    "@types/sinon": "~7.0.0",
    "@types/sinon-chai": "~3.2.1",
    "@types/stringify-object": "~3.2.0",
    "@types/webdriverio": "~4.13.0",
    "@typescript-eslint/eslint-plugin": "~1.9.0",
    "babel-loader": "~8.0.4",
    "babel-plugin-istanbul": "~5.1.0",
    "chai": "~4.2.0",
    "codecov": "~3.5.0",
    "css-loader": "~2.1.0",
    "dom-testing-library": "~4.1.0",
    "eslint": "~5.16.0",
    "eslint-plugin-react": "~7.13.0",
    "fork-ts-checker-webpack-plugin": "~1.3.0",
    "html-webpack-plugin": "~3.2.0",
    "husky": "~2.3.0",
    "ignore-styles": "~5.0.1",
    "jquery": "~3.4.0",
    "jsdom": "~15.1.0",
    "less": "~3.9.0",
    "less-loader": "~5.0.0",
    "lint-staged": "~8.1.0",
    "mocha": "~6.1.0",
    "mugshot": "~0.3.2",
    "mugshot-looks-same": "~1.0.0",
    "mugshot-webdriverio": "~1.0.0",
    "nyc": "~14.1.0",
    "react": "~16.8.1",
    "react-cosmos": "~4.8.0",
    "react-cosmos-shared": "~4.8.0",
    "react-cosmos-voyager2": "~4.8.0",
    "react-dom": "~16.8.1",
    "react-mock-component": "~1.0.1",
    "reset.css": "~2.0.2",
    "sinon": "~7.3.0",
    "sinon-chai": "~3.3.0",
    "style-loader": "~0.23.1",
    "ts-loader": "~6.0.0",
    "ts-node": "~8.1.0",
    "typemoq": "~2.1.0",
    "typescript": "~3.4.0",
    "webdriverio": "~4.14.1",
    "webpack": "~4.32.0",
    "webpack-cli": "~3.3.0",
    "webpack-dev-server": "~3.4.0"
  }
}
