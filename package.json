{
  "name": "react-connect-state",
  "description": "Simple, type safe and time travelling state management for React",
  "keywords": [
    "react",
    "state",
    "typescript"
  ],
  "version": "4.0.0",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "playground": "cosmos --config playground/cosmos.config.js",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc --project tsconfig.build.json --declaration --outDir ./dist",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext ts,tsx,js .",
    "test": "./tests/run.sh",
    "_test": "tdd-buffet test --config tests/jest.config.js --coverage --runInBand",
    "_test:playground": "docker-compose -f tests/gui/docker-compose.yml up -d --build",
    "_test:selenium": "selenium start",
    "_test:selenium:stop": "selenium stop",
    "_test:selenium:debug": "selenium debug",
    "_test:fixtures": "cd playground && ./get-fixtures.js",
    "report-coverage": "codecov -f tests/results/coverage-final.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx?": "eslint",
    "*.js": "eslint"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:NiGhTTraX/react-connect-state.git"
  },
  "dependencies": {
    "@material-ui/core": "~3.9.2",
    "react-bind-component": "~1.0.1",
    "stringify-object": "~3.3.0"
  },
  "peerDependencies": {
    "react": "^16"
  },
  "devDependencies": {
    "@babel/core": "~7.5.0",
    "@babel/plugin-proposal-class-properties": "~7.5.0",
    "@babel/plugin-proposal-decorators": "~7.4.0",
    "@babel/preset-env": "~7.5.0",
    "@babel/preset-react": "~7.0.0",
    "@babel/preset-typescript": "~7.3.0",
    "@babel/register": "~7.5.0",
    "@nighttrax/eslint-config-tsx": "~2.0.0",
    "@tdd-buffet/react": "~0.2.3",
    "@tdd-buffet/selenium": "~0.4.1",
    "@tdd-buffet/visual": "~0.2.9",
    "@types/fetch-mock": "~7.3.0",
    "@types/node": "~10.12.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "~16.8.2",
    "@types/react-hot-loader": "~4.1.0",
    "@types/sinon": "~7.5.0",
    "@types/sinon-chai": "~3.2.1",
    "@types/stringify-object": "~3.2.0",
    "@typescript-eslint/eslint-plugin": "~2.0.0",
    "babel-loader": "~8.0.4",
    "babel-plugin-istanbul": "~5.2.0",
    "chai": "~4.2.0",
    "codecov": "~3.5.0",
    "css-loader": "~3.2.0",
    "eslint": "~5.16.0",
    "eslint-plugin-react": "~7.14.0",
    "fork-ts-checker-webpack-plugin": "~1.5.0",
    "html-webpack-plugin": "~3.2.0",
    "husky": "~3.0.0",
    "jest": "~24.9.0",
    "less": "~3.10.0",
    "less-loader": "~5.0.0",
    "lint-staged": "~9.2.0",
    "react": "~16.9.0",
    "react-cosmos": "~4.8.0",
    "react-cosmos-shared": "~4.8.0",
    "react-cosmos-voyager2": "~4.8.0",
    "react-dom": "~16.9.0",
    "react-mock-component": "~1.0.1",
    "reset.css": "~2.0.2",
    "sinon": "~7.5.0",
    "sinon-chai": "~3.3.0",
    "style-loader": "~1.0.0",
    "tdd-buffet": "~0.8.2",
    "ts-loader": "~6.0.0",
    "ts-node": "~8.3.0",
    "typemoq": "~2.1.0",
    "typescript": "~3.5.0",
    "webdriverio": "~5.12.0",
    "webpack": "~4.39.0",
    "webpack-cli": "~3.3.0",
    "webpack-dev-server": "~3.8.0"
  }
}
